    "    clean_df = df.drop(columns=['Province/State','Lat','Long']).\\\n",
    "                    iloc[:,np.r_[0:1,40:67]].\\\n",
    "                    groupby('Country/Region').sum().\\\n",
    "                    reset_index()\n",
    "    return clean_df"
